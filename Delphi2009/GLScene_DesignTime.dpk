  GLSceneEdit in '..\Source\GLSceneEdit.pas' {GLSceneEditorForm},
  GLSceneRegister in '..\Source\GLSceneRegister.pas',
  RegisterXCollection in '..\Source\RegisterXCollection.pas',
  FShaderMemo in '..\Source\FShaderMemo.pas' {GLShaderEditor},
  GLSceneFormDesign in '..\Source\GLSceneFormDesign.pas',
  FGUILayoutEditor in '..\Source\FGUILayoutEditor.pas' {layouts_form};

end.
